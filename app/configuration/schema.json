      "title": "keybindings",
      "order": 1
    },
    "platformSpecific": true,
    "items": {
      "type": "object",
      "properties": {
        "keystroke": {
          "type": "string"
        },
        "command": {
          "type": "rawString"
        }
      }
    },
    "defaultValue": {
      "linux": [
        {
          "keystroke": "home",
          "command": "\\x1bOH"
        },
        {
          "keystroke": "end",
          "command": "\\x1bOF"
        },
        {
          "keystroke": "ctrl-backspace",
